            <string>///Player move

//select square
if !instance_exists(obj_begin) &amp;&amp; !help = true &amp;&amp; aidone {

    if mouse_x &lt; 160 &amp;&amp; mouse_y &lt; 192 &amp;&amp; mouse_y &gt; 32 { xx = 80 yy = 112 }
    if mouse_x &gt; 160 &amp;&amp; mouse_x &lt; 320 &amp;&amp; mouse_y &lt; 192 &amp;&amp; mouse_y &gt; 32 { xx = 240 yy = 112 }
    if mouse_x &gt; 320 &amp;&amp; mouse_y &lt; 192 &amp;&amp; mouse_y &gt; 32 { xx = 400 yy = 112 }
    
    if mouse_x &lt; 160 &amp;&amp; mouse_y &gt; 192 &amp;&amp; mouse_y &lt; 352 { xx = 80 yy = 272 }
    if mouse_x &gt; 160 &amp;&amp; mouse_x &lt; 320 &amp;&amp; mouse_y &gt; 192 &amp;&amp; mouse_y &lt; 352 { xx = 240 yy = 272 }
    if mouse_x &gt; 320 &amp;&amp; mouse_y &gt; 192 &amp;&amp; mouse_y &lt; 352 { xx = 400 yy = 272 }
    
    if mouse_x &lt; 160 &amp;&amp; mouse_y &gt; 352 { xx = 80 yy = 432 }
    if mouse_x &gt; 160 &amp;&amp; mouse_x &lt; 320 &amp;&amp; mouse_y &gt; 352 { xx = 240 yy = 432 }
    if mouse_x &gt; 320 &amp;&amp; mouse_y &gt; 352 { xx = 400 yy = 432 }
    
    if mouse_y &lt;= 32 || mouse_x = 160 || mouse_x = 320 || mouse_y = 192 || mouse_y = 352 exit
    
    //check if won or lost
    if position_empty(xx,yy) {
        instance_create(xx,yy,obj_x)
        if global.mode = 0 {
            if check(0,0) &amp;&amp; !help = true{
                var a = instance_create(240,272,obj_end)
                with a image_index = 0
                win += 1
                alarm[1] = 120 * global.spdmod
                exit    
            }
        } else if global.mode = 1 {
            if check(1,0) &amp;&amp; !help = true{
                var a = instance_create(240,272,obj_end)
                with a image_index = 1
                loss += 1
                alarm[1] = 120 * global.spdmod
                exit
            }
        }
        if !instance_exists(obj_end) {
            show_debug_message("alarm set")
            alarm[0] = 60 * global.spdmod
            aidone = false
        }
    }
    global.turn += 1
}
