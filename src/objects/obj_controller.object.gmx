        <arguments>
          <argument>
            <kind>1</kind>
            <string>///AI Brain
if instance_exists(obj_begin) with obj_begin instance_destroy()
if help = true { delayed = true exit }

if global.mode = 1 { //inverted mode
    if global.ai = 2 { //strategic ai
        if start = 0 { //first turn if player starts
            if position_meeting(240,272,obj_x) { wingit(0) started = 0 }
            else { move(0,9) started = 1 }
            start = 2
            global.turn += 1
            exit
        } else if start = 1 { //first turn if enemy starts
            instance_create(240,272,obj_aix)
            started = 0
            start = 2
            global.turn += 1
            exit
        }
        if started = 0 move(1,9) //rest of game if enemy started
        else if started = 1 move(0,9) //rest of game if player started
    } else if global.ai = 1 { //competent 
        if start != 2 { //first turn
            wingit(0)
            start = 2 
            started = 0 
            exit
        } else if started = 0 move(4,9)
    } else if global.ai = 0 wingit(0) //random
} else if global.mode = 0 { //regular mode
    if global.ai = 2 { //strategic 
        if start = 1 { //first turn if enemy starts
            instance_create(400,432,obj_aix)
            started = 0
            start = 2
            global.turn += 1
            exit
        }
        if started = 0 move(2,9) //rest of game
    } else if global.ai = 1 { //competent 
        if start != 2 { //first turn
            wingit(0)
            start = 2
            started = 0
            exit
        }
        if started = 0 { show_debug_message("check") //rest of game
            move(3,1)
            if aidone = false move(3,0)
            if aidone = false { wingit(0) show_debug_message("winging it after checks")}
        }
    } else if global.ai = 0 wingit(0) //random 
}
            
//check if won or lost
if global.mode = 0 {
    if check(0,1) &amp;&amp; !help = true{
        var a = instance_create(240,272,obj_end)
        with a image_index = 1
        loss += 1
        alarm[1] = 120 * global.spdmod
        exit
    }
} else if global.mode = 1 {
    if check(1,0) &amp;&amp; !help = true{
        var a = instance_create(240,272,obj_end)
        with a image_index = 0
        win += 1
        alarm[1] = 120 * global.spdmod
        exit
    }
}

aidone = true
</string>
